<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="rootPane" prefHeight="350.0" prefWidth="400.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="library.assistant.ui.addmember.FXMLAddMemberController">
    <stylesheets>
        <URL value="@fxmladdmember.css" />
      <URL value="@../../resources/commonButton.css" />
    </stylesheets>
   <children>
      <VBox layoutX="10.0" layoutY="10.0" prefHeight="310.0" prefWidth="450.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label fx:id="labelLogin" alignment="CENTER" prefHeight="70.0" prefWidth="400.0" style="-fx-background-color: black;" text="Library Assistant Login" textAlignment="CENTER" textFill="WHITE">
               <font>
                  <Font size="19.0" />
               </font>
            </Label>
            <JFXTextField fx:id="name" labelFloat="true" promptText="Name">
               <VBox.margin>
                  <Insets bottom="9.0" left="10.0" right="10.0" top="20.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField fx:id="id" labelFloat="true" promptText="ID">
               <VBox.margin>
                  <Insets bottom="9.0" left="10.0" right="10.0" top="20.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField fx:id="mobile" labelFloat="true" promptText="Mobile">
               <VBox.margin>
                  <Insets bottom="9.0" left="10.0" right="10.0" top="20.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField fx:id="email" labelFloat="true" promptText="Email">
               <VBox.margin>
                  <Insets bottom="9.0" left="10.0" right="10.0" top="20.0" />
               </VBox.margin>
            </JFXTextField>
            <HBox alignment="CENTER" prefHeight="70.0" prefWidth="450.0">
               <children>
                  <JFXButton fx:id="bAddMember" onAction="#addMember" prefHeight="35.0" prefWidth="100.0" text="Save">
                     <graphic>
                        <FontAwesomeIconView glyphName="SAVE" size="20" style="-fx-fill: white;" />
                     </graphic>
                  </JFXButton>
                  <JFXButton prefHeight="35.0" prefWidth="100.0" text="Cancel">
                     <graphic>
                        <FontAwesomeIconView glyphName="TIMES" size="20" style="-fx-fill: white;" />
                     </graphic>
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </JFXButton>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
